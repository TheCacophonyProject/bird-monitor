<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/RandomSeedText"
            motion:layout_constraintEnd_toStartOf="@+id/RandomSeedButton"
            android:layout_width="219dp"
            android:layout_height="48dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="8dp" />
        <Constraint
            android:layout_height="48dp"
            android:layout_marginTop="8dp"
            android:layout_width="219dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:layout_constraintEnd_toStartOf="@+id/RandomSeedButton"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:id="@+id/RandomSeedText"
            android:layout_marginRight="24dp"
            android:layout_marginEnd="24dp"
            motion:layout_editor_absoluteX="89dp" />
        <Constraint
            android:id="@+id/RandomSeedButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_marginEnd="8dp"
            motion:layout_constraintTop_toTopOf="@+id/editTextNumber"
            android:layout_marginRight="8dp" />
        <Constraint
            android:layout_height="49dp"
            motion:layout_constraintTop_toTopOf="@+id/RandomSeedText"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="49dp"
            motion:layout_constraintStart_toEndOf="@+id/RandomSeedText"
            android:id="@+id/RandomSeedButton"
            motion:layout_constraintHorizontal_bias="0.62" />
        <Constraint
            android:layout_marginEnd="8dp"
            android:layout_height="50dp"
            android:layout_marginRight="8dp"
            motion:layout_constraintHorizontal_bias="0.818"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="85dp"
            motion:layout_constraintStart_toEndOf="@+id/textView15"
            android:id="@+id/swUseFrequentUploads"
            motion:layout_editor_absoluteY="42dp" />
        <Constraint
            android:layout_height="86dp"
            android:layout_marginTop="24dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="239dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView15"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.206" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView15"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="6dp"
            android:id="@+id/textView2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>